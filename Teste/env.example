# Configurações do Node-RED
NODE_ENV=development
PORT=1880

# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=3306
DB_NAME=seu_banco
DB_USER=usuario
DB_PASSWORD=senha

# Configurações da API
API_BASE_URL=http://localhost:8080/api
API_TIMEOUT=30000

# Configurações de Segurança
NODE_RED_USER=admin
NODE_RED_PASSWORD=password123
CREDENTIAL_SECRET=sua_chave_secreta_aqui

# Configurações de Log
LOG_LEVEL=info
LOG_FILE=./logs/node-red.log

# Configurações de CORS
CORS_ORIGIN=*
CORS_METHODS=GET,PUT,POST,DELETE,OPTIONS
CORS_HEADERS=Content-Type,Authorization

# Configurações de Dashboard
DASHBOARD_PATH=/ui
DASHBOARD_TITLE=Dashboard de Integração
